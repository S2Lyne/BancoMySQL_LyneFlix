CREATE PROCEDURE `PROC_CAD_PAPEL` (IN nome_papel VARCHAR(255),
									IN ator INT)
BEGIN
	INSERT INTO `lyne_flix`.`papel`(`nome_papel`, `ator_iz`)
    VALUE (nome_papel, ator);
END$$

