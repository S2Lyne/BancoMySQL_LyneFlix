CREATE PROCEDURE `PROC_CAD_GENERO` (IN tipo_genero VARCHAR(255))
BEGIN
	INSERT INTO `lyne_flix`.`genero`(`tipo_genero`)
    VALUE (tipo_genero);
END$$

