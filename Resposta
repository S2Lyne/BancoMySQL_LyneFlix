CREATE PROCEDURE `PROC_CAD_FILME` (IN nome_filme VARCHAR(255),
                                    IN ano_filme VARCHAR(4),
                                    IN avaliacao INT,
                                    IN diretor INT)
BEGIN
	INSERT INTO `lyne_flix`.`filme`(`nome_filme`, `ano_filme`, `avaliacao`, `diretor`)
    VALUE (nome_filme, ano_filme, avaliacao, diretor);
END$$

