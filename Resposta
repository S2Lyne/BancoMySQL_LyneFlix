DELIMITER $$
CREATE PROCEDURE `PROC_CAD_ELENCO` (IN filme INT,
									IN papel INT)
BEGIN
	INSERT INTO `lyne_flix`.`elenco`(`filme`, `papel`)
    VALUE (filme, papel);
END$$

